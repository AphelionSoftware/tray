{
    "manifest_version": 2,
    "name": "b-rush",
    "version": "0.0.0.0",
    "default_locale": "ja_JP",
    "description": "b-rush is a Chrome extension for managing bookmarks.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icon19.png"
        },
        "default_title": "Ext title",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [ "script.js" ]
        }
    ],
    "options_page": "options.html"
}
